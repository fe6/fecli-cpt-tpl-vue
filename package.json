  "devDependencies": {
    "webpack-cli": "^2.0.14"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue",
      "ts"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest",
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  },
  "lint-staged": {
    "component/**/*.vue": [
      "npm run lint"
    ],
    "component/**/*.ts": [
